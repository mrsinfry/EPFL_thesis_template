	abstract = {This paper introduces TI’s mirror-encapsulated bulk acoustic wave (BAW) resonator technology and two novel system applications. Similar to other BAW resonators, the dualBragg acoustic resonator (DBAR) utilizes piezoelectric aluminum nitride (AlN) thin film between two metal electrodes. However, the DBAR operation requires no cavity on either side of the resonant body. This unique micro-acoustic resonator technology enables cost-effective system integration in mass production. The first system application is a novel network synchronizer based on an integrated ultra-low noise voltage-controlled BAW oscillator (VCBO). When operating as a jitter cleaner, the synthesized output clock rms jitter can be reduced to less than 60 fs (12 kHz – 20 MHz). The second application uses this 2.5 GHz DBARoscillator as a high-frequency reference clock, achieving a ±30 ppm Bluetooth Low Energy (BLE) compliant crystal-less radio.},
	language = {en},
	author = {Yen, Ernest Ting-Ta and Zhang, Benyong and Griffith, Danielle and Martin, Keegan and Chowdhury, Mahmud and Segovia-Fernandez, Jeronimo and Tarsi, Trevor and Goodlin, Brian and Cook, Benjamin and Jackson, Ricky and Labs, Kilby and Clara, Santa},
	pages = {2},
	file = {Yen et al. - High-frequency Reference System Implementations Ut.pdf:C\:\\Users\\Admin\\Zotero\\storage\\VKCV458D\\Yen et al. - High-frequency Reference System Implementations Ut.pdf:application/pdf}
}

@article{kourani_x-band_nodate,
	title = {An {X}-{Band} {Oscillator} {Utilizing} {Overtone} {Lithium} {Niobate} {MEMS} {Resonator} and 65-nm {CMOS}},
	abstract = {This paper presents an 8.6 GHz oscillator utilizing a third asymmetric overtone in a lithium niobate (LiNbO3) resonator for 5G communications. The oscillator consists of an acoustic resonator in a closed loop with cascaded tuned amplifiers (TAs) built on TSMC RF GP 65 nm CMOS. The TAs’ bandpass response, set by on-chip inductors, satisfies the oscillation conditions for the 3rd overtone while suppresses the fundamental and higher-order resonances. The oscillator achieves a measured phase noise of -56, and -113 dBc/Hz at 1 kHz, and 100 kHz offsets from an 8.6 GHz carrier while consuming 10.2 mW of dc power. The oscillator achieves a figure-of-merit of 203 at 100 kHz offset, surpassing the state-of-the-art EM and overtoned MEMS oscillators. The achieved oscillation frequency is the highest reported to date for a MEMS oscillator.},
	language = {en},
	author = {Kourani, Ali and Yang, Yansong and Gong, Songbin},
	pages = {3},
