	pages = {265--275},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Admin\\Zotero\\storage\\DIU2RIEF\\6957513.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\M2EDFACW\\Segovia-Fernandez et al. - 2015 - Anchor Losses in AlN Contour Mode Resonators.pdf:application/pdf}
